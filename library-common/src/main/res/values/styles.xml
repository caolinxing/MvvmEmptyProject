<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <!--StartActivity Style 冷启动效果-->
    <style name="common_AppThemeStart"  parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="colorAccent">@color/base_red</item>
        <item name="android:typeface">monospace</item>
    </style>
    <declare-styleable name="CircleImageView" tools:ignore="ResourceName">
        <attr name="civ_border_width" format="dimension" />
        <attr name="civ_border_color" format="color" />
        <attr name="civ_border_overlay" format="boolean" />
        <!-- {@deprecated Use civ_circlebackground_color instead.} -->
        <attr name="civ_fill_color" format="color" />
        <attr name="civ_circle_background_color" format="color" />
    </declare-styleable>

    <style name="TabLayoutTextStyle" tools:ignore="ResourceName">
        <item name="android:textSize">18sp</item>
    </style>
</resources>
