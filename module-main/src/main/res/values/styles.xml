<resources xmlns:tools="http://schemas.android.com/tools" xmlns:android="http://schemas.android.com/apk/res/android">
    <!--StartActivity Style 冷启动效果-->
    <style name="AppThemeStart" tools:ignore="ResourceName" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="android:windowBackground">@drawable/main_splash_start</item>
        <item name="android:windowActionBar">false</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowFullscreen">true</item>
    </style>

    <style name="main_bottom_normal_text">
        <item name="android:textColor">#eeeeee</item>
        <item name="android:textSize">14sp</item>
    </style>
    <!--选中的样式-->
    <style name="main_bottom_selected_text">
        <item name="android:textColor">#ee2e2e</item>
        <item name="android:textSize">14sp</item>
    </style>
</resources>
